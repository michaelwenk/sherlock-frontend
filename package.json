{
  "name": "sherlock-frontend",
  "version": "0.1.0",
  "scripts": {
    "start": "vite --host localhost --port 3001 --strictPort --open",
    "build": "rimraf dist && tsc && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 5000",
    "compile": "tsc",
    "clean-install": "rm package-lock.json && rm -rf node_modules && npm i --legacy-peer-deps",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "npm run eslint --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/cytoscape": "^3.19.4",
    "@types/react-cytoscapejs": "^1.2.2",
    "axios": "^0.26.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "nmr-correlation": "^2.2.5",
    "nmrium": "^0.26.0",
    "openchemlib": "^7.4.3",
    "react": ">=17.0.2",
    "react-cytoscapejs": "^1.2.1",
    "react-dom": ">=17.0.2",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^5.1.4",
    "react-mf": "^2.0.1",
    "react-ocl": "^4.3.5",
    "react-split-pane": "^0.1.92"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-react": "^1.3.0",
    "bootstrap": "^5.1.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "prettier": "^2.6.2",
    "react-bootstrap": "^2.2.3",
    "sass": "^1.49.11",
    "typescript": "^4.6.3",
    "vite": "^2.9.1"
  },
  "volta": {
    "node": "16.14.2"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  }
}
